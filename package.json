  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --write 'src/**/*.(ts|js|html|scss)|*.js|*.json'",
    "deploy:web-stg": "aws s3 sync --profile sen-deploy-user --acl public-read ./www s3://sen-webapp-stg && aws s3 cp --profile sen-deploy-user --cache-control \"max-age=0,no-cache,no-store,must-revalidate\" --acl public-read ./www/ngsw-worker.js s3://sen-webapp-stg",
    "deploy:web-live": "aws s3 sync --profile sen-deploy-user --acl public-read ./www s3://sen-webapp && aws s3 cp --profile sen-deploy-user --cache-control \"max-age=0,no-cache,no-store,must-revalidate\" --acl public-read ./www/ngsw-worker.js s3://sen-webapp",
    "invalid:web-stg": "aws cloudfront create-invalidation --distribution-id E3UYZUQV95X8JE --paths \"/*\" --profile sen-deploy-user",
    "invalid:web-live": "aws cloudfront create-invalidation --distribution-id E1WW51YO646YER --paths \"/*\" --profile sen-deploy-user",
    "graphql-codegen": "graphql-codegen src/reactapp/operations.graphql",
    "graphql-codegen:watch": "graphql-codegen -w src/reactapp/operations.graphql"
  },
